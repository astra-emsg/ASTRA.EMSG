    <summary>NTS Topology Suite is a direct-port of all the functionalities offered by JTS Topology Suite</summary>
    <tags>NTS Topology OGC SFS</tags>
    <dependencies>
      <dependency id="GeoAPI" version="1.5.0" />
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System" targetFramework="" />
      <frameworkAssembly assemblyName="System.Core" targetFramework="" />
      <frameworkAssembly assemblyName="System.Xml" targetFramework="Silverlight4.0" />
